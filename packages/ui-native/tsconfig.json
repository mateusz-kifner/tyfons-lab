{
  "extends": "@tyfons-lab/tsconfig/internal-package.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "ES2022"],
    "jsx": "react-native",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["nativewind/types"],
    "checkJs": false
  },
  "include": ["*.ts", "src"],
  "exclude": ["node_modules"]
}
